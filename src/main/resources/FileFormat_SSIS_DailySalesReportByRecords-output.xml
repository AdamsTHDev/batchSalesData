<?xml version="1.0" encoding="UTF-8"?>
<FileFormat fileName="Sales_Report_By_Records.xls" fileType="EXCEL" locale="en-US">
	<DataSet description="Sales_Report_By_Records">
		<DataSheet sheetName="Sales_Report_By_Records">
			<DataCell row="1" column="A" dataType="TEXT" fieldName="fieldA" defaultValue="CampaignName" />
			<DataCell row="1" column="B" dataType="TEXT" fieldName="fieldB" defaultValue="CampaignCode" />
			<DataCell row="1" column="C" dataType="TEXT" fieldName="fieldC" defaultValue="ListlotName" />
			<DataCell row="1" column="D" dataType="TEXT" fieldName="fieldD" defaultValue="Approve Date" />
			<DataCell row="1" column="E" dataType="TEXT" fieldName="fieldE" defaultValue="X-Ref" />
			<DataCell row="1" column="F" dataType="TEXT" fieldName="fieldF" defaultValue="No" />
			<DataCell row="1" column="G" dataType="TEXT" fieldName="fieldG" defaultValue="Customer" />
			<DataCell row="1" column="H" dataType="TEXT" fieldName="fieldH" defaultValue="Product" />
			<DataCell row="1" column="I" dataType="TEXT" fieldName="fieldI" defaultValue="Premium" />
			<DataCell row="1" column="J" dataType="TEXT" fieldName="fieldJ" defaultValue="AFYP" />
			<DataCell row="1" column="K" dataType="TEXT" fieldName="fieldK" defaultValue="ProtectAmount" />
			<DataCell row="1" column="L" dataType="TEXT" fieldName="fieldL" defaultValue="Payment Channel" />
			<DataCell row="1" column="M" dataType="TEXT" fieldName="fieldM" defaultValue="Mode Of Payment" />
			<DataCell row="1" column="N" dataType="TEXT" fieldName="fieldN" defaultValue="QA Status" />
			<DataCell row="1" column="O" dataType="TEXT" fieldName="fieldO" defaultValue="QA Reason Status" />
			<DataCell row="1" column="P" dataType="TEXT" fieldName="fieldP" defaultValue="Sale Date" />
			<DataCell row="1" column="Q" dataType="TEXT" fieldName="fieldQ" defaultValue="TMR Code" />
			<DataCell row="1" column="R" dataType="TEXT" fieldName="fieldR" defaultValue="TSR" />
			<DataCell row="1" column="S" dataType="TEXT" fieldName="fieldS" defaultValue="TMR Code Manager" />
			<DataCell row="1" column="T" dataType="TEXT" fieldName="fieldT" defaultValue="Manager Name" />
			<DataCell row="1" column="U" dataType="TEXT" fieldName="fieldU" defaultValue="Reason Detail" />
			<DataCell row="1" column="V" dataType="TEXT" fieldName="fieldV" defaultValue="ListLot" />
			<DataCell row="1" column="W" dataType="TEXT" fieldName="fieldW" defaultValue="Period" />
			<DataCell row="1" column="X" dataType="TEXT" fieldName="fieldX" defaultValue="Print Date" />
			<DataRecord listSourceName="salesRecord" beginRow="2" endRow="99999">
				<BeginRecordCondition column="E" comparator="EQ" checkValue="X-Ref" />
				<DataCell row="1" column="A" dataType="TEXT" fieldName="campaignName" />
				<DataCell row="1" column="B" dataType="TEXT" fieldName="campaignCode" />
				<DataCell row="1" column="C" dataType="TEXT" fieldName="listLotName" />
				<DataCell row="1" column="D" dataType="TEXT" fieldName="approveDate"/>
				<DataCell row="1" column="E" dataType="TEXT" fieldName="xRefference" />
				<DataCell row="1" column="F" dataType="NUMBER" fieldName="itemNo" dataFormat="0" />
				<DataCell row="1" column="G" dataType="TEXT" fieldName="customerFullName" />
				<DataCell row="1" column="H" dataType="TEXT" fieldName="product" />
				<DataCell row="1" column="I" dataType="NUMBER" dataFormat="#,##0.00" fieldName="premium" />
				<DataCell row="1" column="J" dataType="NUMBER" dataFormat="#,##0.00" fieldName="annualPremium" />
				<DataCell row="1" column="K" dataType="NUMBER" dataFormat="#,##0.00" fieldName="protectAmount" />
				<DataCell row="1" column="L" dataType="TEXT" fieldName="paymentChannel" />
				<DataCell row="1" column="M" dataType="TEXT" fieldName="paymentFrequency" />
				<DataCell row="1" column="N" dataType="TEXT" fieldName="qaStatus" />
				<DataCell row="1" column="O" dataType="TEXT" fieldName="qaReason" />
				<DataCell row="1" column="P" dataType="TEXT" fieldName="saleDate"/>
				<DataCell row="1" column="Q" dataType="TEXT" fieldName="tsrCode" />
				<DataCell row="1" column="R" dataType="TEXT" fieldName="tsrName" />
				<DataCell row="1" column="S" dataType="TEXT" fieldName="supCode" />
				<DataCell row="1" column="T" dataType="TEXT" fieldName="supName" />
				<DataCell row="1" column="U" dataType="TEXT" fieldName="qaReasonDetail" />
				<DataCell row="1" column="V" dataType="TEXT" fieldName="ListLot" />
				<DataCell row="1" column="W" dataType="TEXT" fieldName="Period" />
				<DataCell row="1" column="X" dataType="TEXT" fieldName="Print Date" />
				<EndRecordCondition column="E" comparator="BLANK" />
			</DataRecord>

		</DataSheet>
	</DataSet>
</FileFormat>