<?xml version="1.0" encoding="UTF-8"?>
<FileFormat fileName="QC Reconfirm.xls" fileType="EXCEL" locale="en-US">
	<DataSet description="QC Reconfirm">
		<DataSheet sheetIndex="0">
			<DataCell row="1" column="A" dataType="TEXT" fieldName="fieldA" defaultValue="Campaign"/>
			<DataCell row="1" column="B" dataType="TEXT" fieldName="fieldB" defaultValue="ListLot"/>
			<DataCell row="1" column="C" dataType="TEXT" fieldName="fieldC" defaultValue="X-Ref"/>
			<DataCell row="1" column="D" dataType="TEXT" fieldName="fieldD" defaultValue="Sale Date"/>
			<DataCell row="1" column="E" dataType="TEXT" fieldName="fieldE" defaultValue="Customer Name"/>
			<DataCell row="1" column="F" dataType="TEXT" fieldName="fieldF" defaultValue="TMR Code"/>
			<DataCell row="1" column="G" dataType="TEXT" fieldName="fieldG" defaultValue="TSR Name"/>
			<DataCell row="1" column="H" dataType="TEXT" fieldName="fieldH" defaultValue="Manager"/>
			<DataCell row="1" column="I" dataType="TEXT" fieldName="fieldI" defaultValue="QC Date"/>
			<DataCell row="1" column="J" dataType="TEXT" fieldName="fieldJ" defaultValue="QC ID"/>
			<DataCell row="1" column="K" dataType="TEXT" fieldName="fieldK" defaultValue="Payment"/>
			<DataCell row="1" column="L" dataType="TEXT" fieldName="fieldL" defaultValue="Premium"/>
			<DataCell row="1" column="M" dataType="TEXT" fieldName="fieldM" defaultValue="TSRStatus"/>
			<DataCell row="1" column="N" dataType="TEXT" fieldName="fieldN" defaultValue="QCStatus"/>
			<DataCell row="1" column="O" dataType="TEXT" fieldName="fieldO" defaultValue="ReconfirmReason"/>
			<DataCell row="1" column="P" dataType="TEXT" fieldName="fieldP" defaultValue="ReconfirmRemark"/>
			<DataCell row="1" column="Q" dataType="TEXT" fieldName="fieldQ" defaultValue="CurrentReason"/>
			<DataCell row="1" column="R" dataType="TEXT" fieldName="fieldR" defaultValue="CurrentRemark"/>
			<DataRecord listSourceName="qcList" beginRow="2" endRow="99999">
				<BeginRecordCondition column="E" comparator="EQ" checkValue="X Ref" />
				<DataCell row="1" column="A" dataType="TEXT" fieldName="campaignName" />
				<DataCell row="1" column="B" dataType="TEXT" fieldName="listLotName" />
				<DataCell row="1" column="C" dataType="TEXT" fieldName="xRefference" />
				<DataCell row="1" column="D" dataType="TEXT" fieldName="saleDate" />
				<DataCell row="1" column="E" dataType="TEXT" fieldName="customerFullName" />
				<DataCell row="1" column="F" dataType="TEXT" fieldName="tsrCode" />
				<DataCell row="1" column="G" dataType="TEXT" fieldName="tsrFullName" />
				<DataCell row="1" column="H" dataType="TEXT" fieldName="supFullName" />
				<DataCell row="1" column="I" dataType="DATE" fieldName="qcStatusDate" dataFormat="dd/MM/yyyy" />
				<DataCell row="1" column="J" dataType="TEXT" fieldName="qcId" />
				<DataCell row="1" column="K" dataType="TEXT" fieldName="paymentMethod" />
				<DataCell row="1" column="L" dataType="NUMBER" fieldName="premium" />
				<DataCell row="1" column="M" dataType="TEXT" fieldName="tsrStatus" />
				<DataCell row="1" column="N" dataType="TEXT" fieldName="qcStatus" />
				<DataCell row="1" column="O" dataType="TEXT" fieldName="reconfirmReason" />
				<DataCell row="1" column="P" dataType="TEXT" fieldName="reconfirmRemark" />
				<DataCell row="1" column="Q" dataType="TEXT" fieldName="currentReason" />
				<DataCell row="1" column="R" dataType="TEXT" fieldName="currentRemark" />
				<EndRecordCondition column="A" comparator="BLANK" />
			</DataRecord>
		</DataSheet>
	</DataSet>
</FileFormat>